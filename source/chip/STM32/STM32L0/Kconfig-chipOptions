config CHIP_STM32_DMAV1_DMA1_CHANNEL7_MAPPING_ENABLE
	bool "DMA1 Channel7 mapping"
	default n
	depends on CHIP_DMAV1_HAS_DMA1_CHANNEL7_MAPPING
	help
