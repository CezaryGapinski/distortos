if CHIP_STM32_USARTV1

config CHIP_STM32_USARTV1_USART1_ENABLE
	bool "USART1 low-level driver"
	default n
	depends on CHIP_STM32_USARTV1_HAS_USART1
	help
		Enable USART1 low-level driver

if CHIP_STM32_USARTV1_USART1_ENABLE

config CHIP_STM32_USARTV1_USART1_DMA_ENABLE
	bool "DMA mode for USART1"
	default n
	depends on CHIP_STM32_USARTV1_HAS_DMA
	help
		Enable DMA mode for USART1 low-level driver

endif	# CHIP_STM32_USARTV1_USART1_ENABLE

