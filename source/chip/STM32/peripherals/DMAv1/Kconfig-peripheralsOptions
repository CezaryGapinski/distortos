if CHIP_STM32_DMAV1

config CHIP_STM32_DMAV1_DMA1_ENABLE
	bool "DMA1 low-level driver"
	default n
	depends on CHIP_STM32_DMAV1_HAS_DMA1
	help
		Enable DMA1 low-level driver
		
config CHIP_STM32_DMAV1_DMA2_ENABLE
	bool "DMA2 low-level driver"
	default n
	depends on CHIP_STM32_DMAV1_HAS_DMA2
	help
		Enable DMA2 low-level driver

if CHIP_STM32_DMAV1_DMA1_ENABLE

config CHIP_STM32_DMAV1_DMA1_CHANNEL1_ENABLE
	bool "DMA1 channel 1 low-level driver"
	default n
	depends on CHIP_STM32_DMAV1_HAS_DMA1_CHANNEL1
	help
		Enable DMA1 channel 1 low-level driver

config CHIP_STM32_DMAV1_DMA1_CHANNEL1_PRIORITY_LEVEL
	int "DMA1 channel 1 priority level"
	depends on CHIP_STM32_DMAV1_DMA1_CHANNEL1_ENABLE
	default 0
	range 0 3
	help
		Level range
		
		0 - Low
		1 - Medium
		2 - High
		3 - Very high

